{
  "name": "koa-spdy-push",
  "description": "SPDY Push helper for Koa",
  "version": "0.3.2",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "koajs/spdy-push",
  "dependencies": {
    "compressible": "2",
    "mime-types": "2",
    "bluebird": "2",
    "destroy": "1",
    "debug": "*",
    "bytes": "1"
  },
  "devDependencies": {
    "co": "3",
    "mz": "*",
    "koa": "*",
    "raw-body": "*",
